{
	"private": true,
	"name": "feedbox",
	"version": "0.0.0",
	"type": "module",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "6.6.0",
		"@typescript-eslint/parser": "6.6.0",
		"esbuild": "0.19.2",
		"eslint": "8.48.0",
		"eslint-config-prettier": "9.0.0",
		"prettier": "3.0.3",
		"spectre.css": "0.5.9",
		"svelte": "4.2.0",
		"typescript": "5.2.2"
	},
	"pnpm": {
		"patchedDependencies": {}
	},
	"prettier": {
		"printWidth": 80,
		"tabWidth": 4,
		"useTabs": true,
		"semi": false,
		"singleQuote": false,
		"quoteProps": "as-needed",
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"bracketSameLine": false,
		"arrowParens": "always",
		"proseWrap": "preserve",
		"htmlWhitespaceSensitivity": "ignore",
		"endOfLine": "lf",
		"embeddedLanguageFormatting": "auto",
		"singleAttributePerLine": true
	}
}
