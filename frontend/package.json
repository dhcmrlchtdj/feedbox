{
    "private": true,
    "name": "feedbox",
    "version": "0.0.0",
    "type": "module",
    "dependencies": {
        "solid-js": "1.4.8"
    },
    "devDependencies": {
        "@babel/core": "7.18.10",
        "@babel/preset-typescript": "7.18.6",
        "babel-preset-solid": "1.4.8",
        "esbuild": "0.14.53",
        "prettier": "2.7.1",
        "typescript": "4.7.4"
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": false,
        "singleQuote": true,
        "quoteProps": "as-needed",
        "jsxSingleQuote": false,
        "trailingComma": "all",
        "bracketSpacing": true,
        "jsxBracketSameLine": true,
        "arrowParens": "always",
        "proseWrap": "preserve",
        "htmlWhitespaceSensitivity": "ignore",
        "endOfLine": "lf",
        "embeddedLanguageFormatting": "auto"
    }
}
