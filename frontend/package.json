{
	"private": true,
	"name": "feedbox",
	"version": "0.0.0",
	"type": "module",
	"devDependencies": {
		"esbuild": "0.20.2",
		"oxlint": "0.3.1",
		"prettier": "3.2.5",
		"prettier-plugin-organize-imports": "3.2.4",
		"spectre.css": "0.5.9",
		"svelte": "5.0.0-next.136",
		"typescript": "5.4.5"
	},
	"pnpm": {
		"patchedDependencies": {}
	},
	"prettier": {
		"printWidth": 80,
		"tabWidth": 4,
		"useTabs": true,
		"semi": false,
		"singleQuote": false,
		"quoteProps": "as-needed",
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"bracketSameLine": false,
		"arrowParens": "always",
		"proseWrap": "preserve",
		"htmlWhitespaceSensitivity": "ignore",
		"endOfLine": "lf",
		"embeddedLanguageFormatting": "auto",
		"singleAttributePerLine": true,
		"plugins": [
			"prettier-plugin-organize-imports"
		],
		"organizeImportsSkipDestructiveCodeActions": true
	}
}
