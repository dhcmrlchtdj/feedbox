{
    "private": true,
    "name": "feedbox",
    "version": "0.0.0",
    "type": "module",
    "devDependencies": {
        "esbuild": "0.12.25",
        "prettier": "2.3.2",
        "svelte": "3.42.4",
        "typescript": "4.4.2"
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": false,
        "singleQuote": true,
        "quoteProps": "as-needed",
        "jsxSingleQuote": false,
        "trailingComma": "all",
        "bracketSpacing": true,
        "jsxBracketSameLine": true,
        "arrowParens": "always",
        "proseWrap": "preserve",
        "htmlWhitespaceSensitivity": "ignore",
        "vueIndentScriptAndStyle": true,
        "endOfLine": "lf",
        "embeddedLanguageFormatting": "auto"
    }
}
